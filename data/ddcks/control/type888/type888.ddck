EQUATIONS 7
BoHS = [2,1]!*NOT(isSummer)     ! Boolean for heating season on, 0/1
isSummer = GE(MOD(time,8760),4344)*LE(MOD(time,8760),5832)! 
HpForDHWIsNeeded = [2,2]
