***********************************
** inputs from hydraulic solver
***********************************

EQUATIONS 4
ThpEvapInCool = TPiHpCondIn     !@connector   
MfrEvapInCool = ABS(MfrPiHpCondIn)     
THpCondInCool = TAmb 
MfrCondInCool = ABS(MfrHpEvapNom)!*0.5  !SCJ: reduce it, to have less switching...

***********************************
** outputs to hydraulic solver
***********************************
EQUATIONS 2
THpEvapOutCool = [162,1]     ! 
THpCondOutCool = [162,3]     ! Temperature of the condenser (heat sink) outlet

******************************************************************************************
** outputs to energy balance in kWh
** Following this naming standard : qSysIn_name, qSysOut_name, elSysIn_name, elSysOut_name
******************************************************************************************
EQUATIONS 3
elSysIn_Q_CoolCompD  = PelCoolComp_kW 
!elSysIn_Q_HpBkp  =  PelCoolTot_kW - PelCoolComp_kW -> these are equal to each other, so elSysIn_Q_HpBkp is 0
qSysOut_HpCoolLoss = PCoolDefrost_kW + PCoolLossStart_kW
qSysOut_PcoolCond = PcoolCond_kW


